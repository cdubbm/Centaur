{
    "name": "srlabs/centaur",
    "description": "An opinionated implementation of Cartalyst's Sentinel for Laravel 5",
    "keywords": ["laravel", "sentinel", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Durham",
            "email": "ryan@stagerightlabs.com",
            "homepage": "http://stagerightlabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Centaur\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.4.*",
        "cartalyst/sentinel": "^2.0.13"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "3.3.*",
        "Mockery/Mockery": "^0.9.4",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Centaur\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "vinkla/hashids": "A Hashids bridge for Laravel 5"
    }
}
